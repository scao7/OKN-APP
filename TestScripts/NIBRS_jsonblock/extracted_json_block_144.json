{
    "entities": [
        {
            "entity": "ResponseCategory",
            "description": "Categories that respondents can choose from in the survey."
        },
        {
            "entity": "UnweightedFrequency",
            "description": "The raw count of responses without any statistical weighting applied."
        },
        {
            "entity": "Percentage",
            "description": "The percentage representation of each response category in the survey."
        },
        {
            "entity": "ValidCases",
            "description": "Total number of cases considered valid for analysis after exclusions."
        },
        {
            "entity": "TotalCases",
            "description": "The total number of all cases, including both valid and invalid."
        },
        {
            "entity": "Mean",
            "description": "The average value for a variable across all respondents."
        },
        {
            "entity": "VariableType",
            "description": "Describes the type of the variable, e.g., numeric."
        },
        {
            "entity": "StandardDeviation",
            "description": "A measure of the amount of variation or dispersion in a set of values."
        },
        {
            "entity": "MinimumValue",
            "description": "The smallest value in a set of responses."
        },
        {
            "entity": "MaximumValue",
            "description": "The largest value in a set of responses."
        },
        {
            "entity": "Location",
            "description": "The data column location of the variable within a dataset."
