{
    "entities": [
        {
            "entity": "Total",
            "description": "The total number of valid cases in the survey dataset."
        },
        {
            "entity": "Frequency",
            "description": "The number of occurrences of a particular value."
        },
        {
            "entity": "UnweightedFrequency",
            "description": "The raw count of occurrences without any weights applied."
        },
        {
            "entity": "Maximum",
            "description": "The maximum value recorded for the variable."
        },
        {
            "entity": "Minimum",
            "description": "The minimum value recorded for the variable."
        },
        {
            "entity": "VariableType",
            "description": "The type of variable, which is numeric in this context."
        },
        {
            "entity": "MissingValues",
            "description": "The range of values indicating missing data."
        }
    ],
    "relationships": [
        {
            "relationship": "hasTotalCases",
            "source_entity": "Survey",
            "target_entity": "Total",
            "description": "Relates the survey to the total number of valid cases."
        },
        {
            "relationship": "hasFrequencyDistribution",
            "source_entity": "Variable",
            "target_entity": "Frequency",
            "description": "Indicates the frequency distribution of the variable's values."
        },
        {
            "relationship": "hasUnweightedFrequencyDistribution",
            "source_entity": "Variable",
            "target_entity": "UnweightedFrequency",
            "description": "Indicates the unweighted frequency distribution of the variable's values."
        },
        {
            "relationship": "hasMaximumValue",
            "source_entity": "Variable",
            "target_entity": "Maximum",
            "description": "Indicates the maximum value recorded for the variable."
        },
        {
            "relationship": "hasMinimumValue",
            "source_entity": "Variable",
            "target_entity": "Minimum",
            "description": "Indicates the minimum value recorded for the variable."
        },
        {
            "relationship": "hasVariableType",
            "source_entity": "Variable",
            "target_entity": "VariableType",
            "description": "Indicates the type of the variable."
        },
        {
            "relationship": "hasMissingValuesRange",
            "source_entity": "Variable",
            "target_entity": "MissingValues",
            "description": "Indicates the range of values that denote missing data."
        }
    ]
}