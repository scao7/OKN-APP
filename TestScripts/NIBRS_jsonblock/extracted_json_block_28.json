{
    "entities": [
        {
            "entity": "ResponseCategory",
            "description": "Categories representing various survey responses."
        },
        {
            "entity": "UnweightedFrequency",
            "description": "Number of times each response was selected, not adjusted for any sampling weights."
        },
        {
            "entity": "Percentage",
            "description": "Percentage of respondents selecting each response category."
        },
        {
            "entity": "TotalCases",
            "description": "Total number of survey responses."
        },
        {
            "entity": "Mean",
            "description": "Average value of the survey responses."
        },
        {
            "entity": "Median",
            "description": "Middle value of the survey responses when ordered."
        },
        {
            "entity": "Mode",
            "description": "Most frequently occurring value in the survey responses."
        },
        {
            "entity": "Minimum",
            "description": "Smallest value in the survey responses."
        },
        {
            "entity": "Maximum",
            "description": "Largest value in the survey responses."
        },
        {
            "entity": "StandardDeviation",
            "description": "Measure of the amount of variation or dispersion in the survey responses."
        },
        {
            "entity": "Location",
            "description": "Position of the variable in the dataset; includes starting and ending column numbers, width, and decimal places."
        },
        {
            "entity": "VariableType",
            "description": "Data type of the variable in the dataset."
        }
    ],
    "relationships": [
        {
