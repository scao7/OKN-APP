{
    "entities": [
        {
            "entity": "OffenderNumber",
            "description": "Identifier for offender within the survey."
        },
        {
            "entity": "Frequency",
            "description": "Unweighted frequency count of occurrences."
        },
        {
            "entity": "SurveyResponse",
            "description": "Response given by the survey participant."
        },
        {
            "entity": "MissingData",
            "description": "Proportion of the dataset with missing information."
        },
        {
            "entity": "DistributionStatistics",
            "description": "Statistical measures of the dataset (mean, median, mode, etc.)."
        }
    ],
    "relationships": [
        {
            "relationship": "hasOffenderNumber",
            "source_entity": "Survey",
            "target_entity": "OffenderNumber",
            "description": "Associates the survey with the offender's unique identifier."
        },
        {
            "relationship": "hasFrequency",
            "source_entity": "OffenderNumber",
            "target_entity": "Frequency",
            "description": "Indicates the count of responses associated with an offender number."
        },
        {
            "relationship": "hasSurveyResponse",
            "source_entity": "Survey",
            "target_entity": "SurveyResponse",
            "description": "Captures the response provided by the survey participant."
        },
        {
            "relationship": "hasMissingData",
            "source_entity": "SurveyData",
            "target_entity": "MissingData",
            "description": "Indicates the proportion or segments of the dataset that contain missing data."
        },
        {
            "relationship": "hasDistributionStatistics",
            "source_entity": "SurveyData",
            "target_entity": "DistributionStatistics",
            "description": "Summarizes the statistical distribution measures of the data."
        }
    ]
}